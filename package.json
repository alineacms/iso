    "@types/node": "^18.0.0",
    "esbuild": "^0.14.43",
    "esbx": "^0.0.20",
    "typescript": "^4.7.3",
    "@remix-run/web-fetch": "^4.1.3",
    "@remix-run/web-file": "^3.0.2"
  },
  "scripts": {
    "build": "tsc && node build.js",
    "prepare": "tsc && node build.js"
  },
  "resolutions": {
    "data-uri-to-buffer": "^4.0.0"
  }
